{% extends 'base.html.twig' %}

{% block title %}Créer un projet - {{ parent() }}{% endblock %}
{% block header %}<h1>Créer un projet</h1>{% endblock %}

{% block body %}
    <div id="content">
        {{ form_start(form) }}
            <div>
                {{ form_row(form.name) }}
            </div>
            <div>
                {{ form_row(form.users) }}       
            </div>
            <br>
            <button class="button button-submit">Continuer</button>
        {{ form_end(form) }}
    </div>
{% endblock %}